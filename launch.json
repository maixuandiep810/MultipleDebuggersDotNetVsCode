    "version": "0.2.0",
    "configurations": [
        {
            "name": "Backend API .NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/marketplace.src/PresentationLayer/marketplace.BackendApi/bin/Debug/netcoreapp3.1/marketplace.BackendApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/marketplace.src/PresentationLayer/marketplace.BackendApi",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "WEB APP .NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/marketplace.src/PresentationLayer/marketplace.WebApp/bin/Debug/netcoreapp3.1/marketplace.WebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/marketplace.src/PresentationLayer/marketplace.WebApp",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "ADMIN APP .NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build3",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/marketplace.src/PresentationLayer/marketplace.AdminApp/bin/Debug/netcoreapp3.1/marketplace.AdminApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/marketplace.src/PresentationLayer/marketplace.AdminApp",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
